    while true do
        if not rotating then
            return 
        end
        hourglass_full.Rotation = hourglass_full.Rotation + 2
        wait()
    end
end)
coroutine.resume(rotate)
wait(2)
done.ImageColor3 = Color3.fromRGB(0, 255, 0)
