color(0, 255, 0)
local jjke = coroutine.create(function()
    wait(0.3)
    ImgT(done, 0)
    wait(1)
end)
coroutine.resume(jjke)
colors(Checking, 0, 255, 0, "TextColor3")
typing(Checking, "You're Whitelisted," .. plr.Name)
